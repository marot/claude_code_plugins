{
  "name": "@maestro/scripts",
  "version": "1.0.0",
  "description": "CLI tools for Maestro mobile automation - designed for Claude Code",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "maestro-scripts": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "maestro",
    "mobile",
    "testing",
    "automation",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "README.md"
  ]
}
